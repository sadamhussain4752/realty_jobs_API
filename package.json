    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "events": "^3.3.0",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.7.0",
