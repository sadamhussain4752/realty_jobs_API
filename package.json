{
  "name": "winterbearapi",
  "version": "1.0.0",
  "description": "A Node.js server using Express, MongoDB, and Mongoose",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "nodemon src/app.js",
    "test": "mocha tests/**/*.js",
